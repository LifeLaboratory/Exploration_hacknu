;AAQA,KATA;AAUA,IAAA,QAVA,sBAUA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,WAAA,EADA;;AAAA;AAEA,gBAAA,MAAA,CAAA,IAAA,GAAA,IAAA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGA,KAbA;AAcA,IAAA,SAdA,qBAcA,GAdA,EAcA;AACA,WAAA,EAAA,CAAA,GAAA,GAAA,GAAA;AACA,WAAA,WAAA;AACA,KAjBA;AAkBA,IAAA,aAlBA,yBAkBA,GAlBA,EAkBA;AACA,WAAA,EAAA,CAAA,QAAA,GAAA,GAAA;AACA,WAAA,WAAA;AACA;AArBA;AAlBA,CAAA","sourcesContent":["<template>\n    <div class=\"scroll\">\n        <div class=\"avatar\"> \n            <img\n                :src=\"me.avatar\"\n            />\n        </div>\n\n        <div class=\"name\">\n            <div>{{me.name}}</div>\n        </div>\n        <div style=\"margin-top: 10px\">\n            <h2><b>Выберите ваш пол</b></h2>\n            <b-button size=\"is-medium\"\n                style=\"margin-top: 10px;\"\n                :class=\"me.sex === true ? 'is-primary' : ''\"\n                @click=\"changeSex(true)\"\n                icon-left=\"male\">\n                Мужчина\n            </b-button>\n            <b-button size=\"is-medium\"\n                :class=\"me.sex === false ? 'is-primary' : ''\"\n                style=\"margin-top: 10px;\"\n                @click=\"changeSex(false)\"\n                icon-left=\"female\">\n                Девушка\n            </b-button>\n        </div>\n\n        <div style=\"margin-top: 30px;\">\n            <h2><b>Выберите пол желаемого партнера</b></h2>\n            <b-button size=\"is-medium\"\n                style=\"margin-top: 10px;\"\n                :class=\"me.sex_find === true ? 'is-primary' : ''\"\n                @click=\"changeSexFind(true)\"\n                icon-left=\"male\">\n                Мужчина\n            </b-button>\n            <b-button size=\"is-medium\"\n                style=\"margin-top: 10px;\"\n                :class=\"me.sex_find === false ? 'is-primary' : ''\"\n                @click=\"changeSexFind(false)\"\n                icon-left=\"female\">\n                Девушка\n            </b-button>\n        </div>\n        <div>\n            <b-field label=\"О себе\" style=\"width: 80%; margin-left: 10%; margin-top: 20px\">\n                <b-input maxlength=\"100\" v-model=\"me.description\" type=\"textarea\"></b-input>\n            </b-field>\n        </div>\n        <div class=\"greenColors\" v-if=\"show\"> Настройки успешно сохранены !</div>\n        <br />\n        <b-button @click=\"saveProf()\" icon-left=\"save\" style=\"background: #a31f63; color: white\">\n            <b>Сохранить</b>\n        </b-button>\n    </div>\n</template>\n\n<script>\nimport {ip} from '@/cfg/setting.js'\nimport axios from 'axios'\nexport default {\n    data() {\n        return {\n            me: {},\n            show: false,\n            desc: '123'\n        }\n    },\n    mounted: async function () {\n        let d = await axios.get(`${ip}/user/profile`,\n        {\n            headers: {\n            'session': localStorage.getItem('id')\n            }\n        })\n        this.me = d.data[0]\n\n    },\n    methods: {\n        async snedNewInfo() {\n            let d = await axios.post(`${ip}/user/profile`,\n            this.me,\n            {\n                headers: {\n                'session': localStorage.getItem('id')\n                }\n            })\n        },\n        async saveProf() {\n            await this.snedNewInfo()\n            this.show = true\n        },\n        changeSex(sex) {\n            this.me.sex = sex\n            this.snedNewInfo()\n        },\n        changeSexFind(sex) {\n            this.me.sex_find = sex\n            this.snedNewInfo()\n        }\n    }\n}\n</script>\n<style scoped>\n    .avatar {\n        margin: auto;\n    }\n\n    .avatar img {\n        margin-top: 12px;\n        border-radius: 100px;\n        width: 120px;\n        height: 120px;\n    }\n\n    .name {\n        font-size: 24px;\n    }\n\n    .buttons {\n        margin: auto;\n        margin-top: 30px;\n        max-width: 700px;\n    }\n\n    .buttons button {\n        border-radius: 100px;\n        height: 45px;\n        width: 45px;\n    }\n\n    .text {\n        font-size: 15px;\n    }\n\n    .scroll {\n        overflow-y: auto;\n    }\n\n    .greenColors {\n        color: green\n    }\n</style>"],"sourceRoot":"src/views"}]}